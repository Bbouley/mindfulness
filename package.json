{
  "name": "bmindful",
  "version": "1.0.0",
  "description": "Simple logging and metrics interfaces",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest",
    "test-watch": "jest --watch",
    "build": "tsc",
    "build-watch": "tsc --watch"
  },
  "keywords": [
    "logging",
    "metrics"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^22.0.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-jest": "^22.1.0",
    "babel-preset-env": "^1.6.1",
    "eslint": "^4.15.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0",
    "express": "^4.16.2",
    "jest": "^22.1.1",
    "nock": "^9.1.6",
    "sinon": "^4.1.5",
    "ts-jest": "^22.0.1",
    "ts-node": "^4.1.0",
    "typescript": "^2.6.2"
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {
      ".ts": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/test/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "testPathIgnorePatterns": [
      "<rootDir>/dist/"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "dependencies": {
    "request": "^2.83.0",
    "request-promise-native": "^1.0.5"
  }
}
